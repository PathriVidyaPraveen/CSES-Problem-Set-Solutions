#include<bits/stdc++.h>
using namespace std;

int result = 0;
bool col[8];
bool diag1[15]; // row - col + 7
bool diag2[15]; // row + col
vector<string> board(8);

void dfs(int row){
    if(row == 8){
        result++;
        return;
    }
    for(int c=0;c<8;c++){
        if(board[row][c]=='*'){
            continue;
        }
        if(col[c]){
            continue;
        }
        if(diag1[row-c+7]){
            continue;
        }
        if(diag2[row+c]){
            continue;
        }
        col[c]=diag1[row-c+7]=diag2[row+c] = true;
        dfs(row+1);
        col[c]=diag1[row-c+7]=diag2[row+c] = false;
    }
}

int main(){

    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    for(int i=0;i<8;i++){
        cin >> board[i];
    }

    dfs(0);
    cout << result << endl;

    
    
    return 0;
}
